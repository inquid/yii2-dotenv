{
    "name": "inquid/yii2-dotenv",
    "description": "PHP DotEnv for Yii2 framework",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","dotenv"],
    "license": "MIT",
    "authors": [
        {
            "name": "Panlatent",
            "email": "panlatent@gmail.com"
        },
        {
            "name": "Industrial Quality Ideas LLC",
            "email": "contact@inquid.co"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "*",
        "vlucas/phpdotenv": "^5.3.0"
    },
    "autoload": {
        "psr-4": {
            "yiithings\\dotenv\\": "src/"
        },
        "files": ["src/autoload.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
